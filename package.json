{
  "author": "Nicholas Jamieson <nicholas@cartant.com>",
  "bugs": {
    "url": "https://github.com/cartant/firebase-key/issues"
  },
  "dependencies": {},
  "description": "Firebase key utility and encoding/decoding functions",
  "devDependencies": {
    "@types/chai": "^3.5.1",
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.13",
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "karma": "^1.6.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-spec-reporter": "^0.0.31",
    "mocha": "^3.2.0",
    "rimraf": "^2.6.1",
    "rollup": "^0.41.6",
    "tslib": "^1.6.1",
    "tslint": "^5.1.0",
    "typescript": "^2.2.2",
    "uglifyjs": "^2.4.10"
  },
  "homepage": "https://github.com/cartant/firebase-key",
  "jsnext:main": "./dist/index.js",
  "keywords": [
    "firebase",
    "key",
    "push",
    "pushid",
    "pushkey",
    "timestamp"
  ],
  "license": "MIT",
  "main": "./bundles/firebase-key.umd.js",
  "module": "./dist/index.js",
  "name": "firebase-key",
  "optionalDependencies": {},
  "peerDependencies": {},
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/cartant/firebase-key.git"
  },
  "scripts": {
    "dist": "npm run dist:build && npm run dist:uglify",
    "dist:build": "npm run dist:clean && tsc -p tsconfig-dist.json && rollup -c rollup-config.js",
    "dist:clean": "rimraf ./dist && rimraf ./bundles/firebase-key.*",
    "dist:uglify": "uglifyjs -c --comments /license/ -m --screw-ie8 -o ./bundles/firebase-key.min.umd.js ./bundles/firebase-key.umd.js",
    "lint": "tslint --project tsconfig.json --type-check source/**/*.ts",
    "prepublish": "npm run test && npm run dist",
    "test": "npm run lint && npm run test:build && karma start",
    "test:build": "npm run test:clean && tsc -p tsconfig.json && rollup -c rollup-config-test.js",
    "test:clean": "rimraf ./build && rimraf ./bundles/firebase-key-test.*"
  },
  "typings": "./dist/index.d.ts",
  "unpkg": "./bundles/firebase-key.min.umd.js",
  "version": "1.0.1"
}
